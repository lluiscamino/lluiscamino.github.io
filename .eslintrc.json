{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "ignorePatterns": [],
  "rules": {
    "max-len": [
      "error",
      80,
      {
        "ignoreTemplateLiterals": true
      }
    ],
    "indent": [
      "error",
      2,
      {
        "ignoredNodes": [
          "TemplateLiteral *"
        ]
      }
    ],
    "require-jsdoc": "off"
  }
}
